{
    "name"             : "oddgreg/nine-library",
    "description"      : "A library of standard utility classes and functions.",
    "type"             : "library",
    "license"          : "MIT",
    "homepage"         : "https://github.com/OddGreg/NineLibrary",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "autoload"         : {
        "classmap"             : [
            "tests",
            "src"
        ],
        "files"                : [
            "src/helpers.php"
        ],
        "exclude-from-classmap": [
        ],
        "psr-4"                : {
            "Nine\\Library\\": "src"
        }
    },
    "prefer-stable"    : true,
    "minimum-stability": "dev",
    "require"          : {
    },
    "require-dev"     : {
        "phpunit/phpunit"              : "^5.1@dev",
        "psy/psysh"                    : "@stable",
        "tracy/tracy"                  : "^2.4@dev",
        "codeclimate/php-test-reporter": "dev-master"
    }
}
